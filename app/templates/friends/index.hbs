<table class="primary">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |friend|}}
      <tr>
        <td>
          {{link-to friend.fullName 'articles' friend}}
        </td>
        <td>
          {{friend.email}}
        </td>
        <td>
          {{link-to 'Lend article' 'articles.new' friend}}
        </td>
        <td>
          <a href="#"{{action "delete" friend}}>Delete</a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
